{
  "name": "@lapidist/dtif-schema",
  "version": "1.0.6",
  "description": "JSON Schema for the Design Token Interchange Format (DTIF).",
  "type": "module",
  "license": "MIT",
  "main": "core.json",
  "types": "index.d.ts",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./core.json"
    },
    "./core": "./core.json",
    "./core.json": "./core.json",
    "./index.d.ts": "./index.d.ts"
  },
  "files": [
    "core.json",
    "index.d.ts",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "design-tokens",
    "dtif",
    "json",
    "schema"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bylapidist/dtif.git",
    "directory": "schema"
  },
  "publishConfig": {
    "access": "public"
  }
}
