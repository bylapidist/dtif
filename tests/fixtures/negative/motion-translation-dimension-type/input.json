{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "angles": {
    "quarterTurn": {
      "$type": "dimension",
      "$value": { "dimensionType": "angle", "value": 90, "unit": "deg" }
    }
  },
  "problem": {
    "$type": "motion",
    "$value": {
      "motionType": "css.translate",
      "parameters": {
        "x": { "$ref": "#/angles/quarterTurn" }
      }
    }
  }
}
