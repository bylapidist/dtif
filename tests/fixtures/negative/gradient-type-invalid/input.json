{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "gradient": {
    "bad": {
      "$type": "gradient",
      "$value": {
        "gradientType": "diagonal",
        "stops": [
          {
            "position": 0,
            "color": {
              "colorSpace": "srgb",
              "components": [0, 0, 0, 1]
            }
          },
          {
            "position": 1,
            "color": {
              "colorSpace": "srgb",
              "components": [1, 1, 1, 1]
            }
          }
        ]
      }
    }
  }
}
