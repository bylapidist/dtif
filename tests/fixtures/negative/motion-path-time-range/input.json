{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "problem": {
    "$type": "motion",
    "$value": {
      "motionType": "css.offset-path",
      "parameters": {
        "points": [
          {
            "time": 0,
            "position": {
              "x": { "dimensionType": "length", "value": 0, "unit": "px" }
            }
          },
          {
            "time": 0.5,
            "position": {
              "x": { "dimensionType": "length", "value": 24, "unit": "px" }
            }
          },
          {
            "time": 1.5,
            "position": {
              "x": { "dimensionType": "length", "value": 48, "unit": "px" }
            }
          }
        ]
      }
    }
  }
}
