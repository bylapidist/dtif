{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "problem": {
    "$type": "motion",
    "$value": {
      "motionType": "css.rotate3d",
      "parameters": {
        "axis": {
          "x": 0,
          "y": 0,
          "z": 0
        },
        "angle": { "dimensionType": "angle", "value": 90, "unit": "deg" }
      }
    }
  }
}
