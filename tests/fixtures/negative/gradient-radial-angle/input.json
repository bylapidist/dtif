{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "bad": {
    "$type": "gradient",
    "$value": {
      "gradientType": "radial",
      "stops": [
        {
          "position": 0,
          "color": { "colorSpace": "srgb", "components": [0.2, 0.2, 0.2, 1] }
        },
        {
          "position": 1,
          "color": { "colorSpace": "srgb", "components": [0.8, 0.8, 0.8, 1] }
        }
      ],
      "angle": 270
    }
  }
}
