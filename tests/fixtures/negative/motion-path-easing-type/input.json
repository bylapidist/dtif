{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "palette": {
    "background": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [0.2, 0.3, 0.4, 1]
      }
    }
  },
  "problem": {
    "$type": "motion",
    "$value": {
      "motionType": "css.offset-path",
      "parameters": {
        "points": [
          {
            "time": 0,
            "position": {
              "x": { "dimensionType": "length", "value": 0, "unit": "px" }
            }
          },
          {
            "time": 1,
            "position": {
              "x": { "dimensionType": "length", "value": 16, "unit": "px" }
            },
            "easing": "#/palette/background"
          }
        ]
      }
    }
  }
}
