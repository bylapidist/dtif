{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "bad": {
    "$type": "gradient",
    "$value": {
      "gradientType": "conic",
      "stops": [
        {
          "position": 0,
          "color": { "colorSpace": "srgb", "components": [0.3, 0.3, 0.3, 1] }
        },
        {
          "position": 1,
          "color": { "colorSpace": "srgb", "components": [0.9, 0.9, 0.9, 1] }
        }
      ],
      "shape": "circle"
    }
  }
}
