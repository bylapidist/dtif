{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "color": {
    "brand": { "$type": "color", "$value": { "colorSpace": "srgb", "components": [0, 0, 0, 1] } }
  },
  "$overrides": [
    {
      "$token": "#/color/brand",
      "$when": { "prefers-color-scheme": "dark" },
      "$ref": "#/color/missing"
    }
  ]
}
