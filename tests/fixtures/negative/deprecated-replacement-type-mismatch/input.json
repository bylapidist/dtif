{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "color": {
    "brand": {
      "$type": "color",
      "$value": { "colorSpace": "srgb", "components": [0, 0, 1] }
    },
    "brand-legacy": {
      "$type": "color",
      "$value": { "colorSpace": "srgb", "components": [1, 0, 0] },
      "$deprecated": { "$replacement": "#/opacity/transparent" }
    }
  },
  "opacity": {
    "transparent": {
      "$type": "opacity",
      "$value": { "opacityType": "css.opacity", "value": 0 }
    }
  }
}
