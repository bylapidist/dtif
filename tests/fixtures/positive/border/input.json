{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "focus": {
    "$type": "border",
    "$value": {
      "borderType": "css.border",
      "color": {
        "colorSpace": "srgb",
        "components": [0.0, 0.4, 0.8, 1.0]
      },
      "radius": {
        "bottomRight": {
          "x": {
            "dimensionType": "length",
            "value": 20,
            "unit": "pt"
          }
        },
        "topLeft": {
          "x": {
            "dimensionType": "length",
            "value": 12,
            "unit": "pt"
          },
          "y": {
            "dimensionType": "length",
            "value": 8,
            "unit": "pt"
          }
        }
      },
      "style": "dashed",
      "width": {
        "dimensionType": "length",
        "value": 2,
        "unit": "pt"
      }
    }
  },
  "keyword": {
    "$type": "border",
    "$value": {
      "borderType": "css.border",
      "color": {
        "colorSpace": "srgb",
        "components": [0.2, 0.2, 0.2, 1.0]
      },
      "style": "solid",
      "width": "thin"
    }
  }
}
