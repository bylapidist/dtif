{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "typography": {
    "keyword": {
      "$type": "typography",
      "$value": {
        "typographyType": "body",
        "fontFamily": "Inter",
        "fontWeight": "bold",
        "fontSize": {
          "dimensionType": "length",
          "value": 16,
          "unit": "px"
        }
      }
    },
    "keywordBolder": {
      "$type": "typography",
      "$value": {
        "typographyType": "body",
        "fontFamily": "Inter",
        "fontWeight": "bolder",
        "fontSize": {
          "dimensionType": "length",
          "value": 16,
          "unit": "px"
        }
      }
    }
  }
}
