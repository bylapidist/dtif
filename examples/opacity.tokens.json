{
  "$version": "1.0.0",
  "opacity": {
    "layer": {
      "$type": "opacity",
      "$value": {
        "opacityType": "ios.layer.opacity",
        "value": 0.72
      }
    },
    "overlay": {
      "$type": "opacity",
      "$value": {
        "opacityType": "css.opacity",
        "value": "calc(0.8 * var(--dt-layer-alpha))"
      }
    },
    "overlay-alias": {
      "$type": "opacity",
      "$ref": "#/opacity/overlay"
    },
    "scrim": {
      "$type": "opacity",
      "$value": {
        "opacityType": "android.view.alpha",
        "value": "72%"
      }
    }
  }
}
