{
  "$version": "1.0.0",
  "color": {
    "overlay": {
      "$type": "color",
      "$description": "Translucent overlay with legacy hex fallback",
      "$value": {
        "colorSpace": "srgb",
        "components": [0.0, 0.0, 0.0, 0.5]
      },
      "$extensions": {
        "org.example.export": { "legacyHex": "#00000080" }
      }
    }
  }
}
