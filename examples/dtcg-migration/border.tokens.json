{
  "$version": "1.0.0",
  "border": {
    "focus-outline": {
      "$type": "border",
      "$value": {
        "borderType": "css.border",
        "color": {
          "colorSpace": "srgb",
          "components": [0.0, 0.435, 1.0, 1.0]
        },
        "style": "solid",
        "strokeStyle": { "$ref": "#/strokeStyle/focus" },
        "width": {
          "dimensionType": "length",
          "value": 2,
          "unit": "px"
        }
      }
    }
  },
  "strokeStyle": {
    "focus": {
      "$type": "strokeStyle",
      "$value": {
        "dashArray": [4, 2],
        "lineCap": "round"
      }
    }
  }
}
